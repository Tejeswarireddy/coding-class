import java.util.*;
class Num
{
public static void main(String args[])
{
Scanner sc=new Scanner(System.in);
int num;
num=sc.nextInt();
int n,r=0,sum=0;

n=num;
while(num>0)
{
    r=num%10;
    sum=sum+r;
    num=num/10;
}
  if(n%sum==0)
    {
        System.out.println(n+"is a harshad number");
    
    }
    else
    {
        System.out.println(n+"is not a harshad number");
    }
}

}


